{
    "installCommand": "npm install -g npm-run-all vite && npm install",
    "buildCommand": "vite build",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}